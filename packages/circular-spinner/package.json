{
  "name": "@ats-components/circular-spinner",
  "version": "1.0.8",
  "main": "dist/indexApp.js",
  "homepage": "https://github.com/kasselTrankos/ats-components/tree/master/packages/circular-spinner#readme",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "start": "react-native start",
    "postinstall": "mkdir dist && npx @babel/cli App.js spinner.js --out-dir dist  --presets=@babel/preset-env",
    "test": "jest"
  },
  "command": {
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "expo": "~40.0.0",
    "expo-splash-screen": "~0.8.0",
    "expo-status-bar": "~1.0.3",
    "expo-updates": "~0.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-reanimated": "~1.13.0",
    "react-native-screens": "~2.15.0",
    "react-native-svg": "^12.1.0",
    "react-native-unimodules": "~0.12.0",
    "react-native-web": "~0.13.12"
  },
  "jest": {
    "preset": "react-native"
  }
}
